  tone(10, 293.67,200); delay(500);//D
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 293.67,200); delay(500);//D
  tone(10, 293.67,200); delay(500);//D
  tone(10, 293.67,200); delay(500);//D
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 392,   200); delay(500);//G
  tone(10, 392,   200); delay(500);//G
  
  //mary had a little lamb
  tone(10, 329.6,150); delay(500);//E
  tone(10, 293.67,150); delay(500);//D
  tone(10, 261.6, 200); delay(500);//C
  tone(10, 293.67,200); delay(500);//D
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  
  //whose fleece was white as snow
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 293.67,200); delay(500);//D
  tone(10, 293.67,200); delay(500);//D
  tone(10, 329.6,150); delay(500);//E
  tone(10, 293.67,150); delay(500);//D
  tone(10, 261.6, 400); delay(500);//C
}
void song5()
{
  //ring around the rosie
  tone(10, 392,   100); delay(500);//G
  tone(10, 392,   100); delay(500);//G
  tone(10, 329.6, 150); delay(500);//E
  tone(10, 440.0, 100); delay(500);//A
  tone(10, 392,   150); delay(500);//G
  tone(10, 329.6, 300); delay(500);//E
  
  //a pocket full of posies
  tone(10, 349.23, 100); delay(500);//F
  tone(10, 392,   100); delay(500);//G
  tone(10, 392,   100); delay(500);//G
  tone(10, 329.6,150); delay(500);//E
  tone(10, 440.0, 100); delay(500);//A
  tone(10, 392,   150); delay(500);//G
  tone(10, 329.6,200); delay(500);//E
  
  //ashes, ashes
  tone(10, 392,   200); delay(500);//G
  tone(10, 329.6,150); delay(500);//E
  tone(10, 392,   200); delay(500);//G
  tone(10, 329.6,150); delay(500);//E
  
  //they all fall down
  tone(10, 329.6,100); delay(500);//E
  tone(10, 392,   150); delay(500);//G
  tone(10, 392,   200); delay(500);//G
  tone(10, 261.6, 400); delay(500);//C
}

//MAIN PROGRAM
void loop() {


  Serial.println("Song Selection:");    
  Serial.println("		[1] Jingle Bells");
  Serial.println("		[2] Twinkle, Twinkle, Little Star");
  Serial.println("		[3] Happy Birthday Song");
  Serial.println("		[4] Mary had a Little Lamb");
  Serial.println("		[5] Ring Around the Rosie");
 
while (Serial.available()==0) {}
  
int userinput = Serial.parseInt();

switch (userinput) {
case 1:
song1();
break;

case 2:
song2();
break;
  
case 3:
song3();
break;

case 4:
song4();
break;
  
case 5:
song5();
break;

default:
Serial.println("Selected Feature Does Not Exist");
}
}
