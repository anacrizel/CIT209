  tone(10, 261.6, 400); delay(500);//C
  tone(10, 293.67,300); delay(500);//D
  tone(10, 329.6, 500); delay(500);//E
  //oh what fun it is to ride
  tone(10, 349.23, 200); delay(500);//F
  tone(10, 349.23, 300); delay(500);//F
  tone(10, 349.23, 500); delay(500);//F
  tone(10, 349.23, 200); delay(500);//F
  tone(10, 349.23, 300); delay(500);//F
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 300); delay(500);//E
  tone(10, 329.6, 500); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
   //in a one horse open slay
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 293.67,300); delay(500);//D
  tone(10, 293.67,300); delay(500);//D
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 293.67,600); delay(500);//D
  tone(10, 392,   700); delay(500);//G
  
  //Jingle bells, jingle bells, jingle all the way
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 300); delay(500);//E
  tone(10, 329.6, 400); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 300); delay(500);//E
  tone(10, 329.6, 400); delay(500);//E
  tone(10, 329.6, 500); delay(500);//E
  tone(10, 392,   700); delay(500);//G
  tone(10, 261.6, 400); delay(500);//C
  tone(10, 293.67,300); delay(500);//D
  tone(10, 329.6, 500); delay(500);//E
  
  //oh what fun it is to ride
  tone(10, 349.23, 200); delay(500);//F
