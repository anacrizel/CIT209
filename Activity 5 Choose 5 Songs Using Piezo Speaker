void setup()
{
 pinMode(10, OUTPUT);
 Serial.begin(9600);
}

void song1()
{
  //Jingle bells, jingle bells, jingle all the way
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 300); delay(500);//E
  tone(10, 329.6, 400); delay(500);//E
  tone(10, 329.6, 200); delay(500);//E
  tone(10, 329.6, 300); delay(500);//E
  tone(10, 329.6, 400); delay(500);//E
  tone(10, 329.6, 500); delay(500);//E
