void song3()
{
  //happy birthday to you
  tone(10, 587.33, 100); delay(500);//D/
  tone(10, 587.33, 100); delay(500);//D/
  tone(10, 659.3, 200); delay(500);//E/
  tone(10, 587.33, 200); delay(500);//D/
  tone(10, 784.0, 300); delay(500);//G/
  tone(10, 740.0, 400); delay(500);//F#
  
  //happy birthday to you
  tone(10, 587.33, 100); delay(500);//D/
  tone(10, 587.33, 100); delay(500);//D/
  tone(10, 659.3, 200); delay(500);//E/
  tone(10, 587.33, 200); delay(500);//D/
  tone(10, 784.0, 300); delay(500);//G/
  tone(10, 740.0, 400); delay(500);//F#
  
  //happy birthday
  tone(10, 587.33, 100); delay(500);//D/
  tone(10, 587.33, 200); delay(500);//D/
  tone(10, 1006.5, 300); delay(500);//C/
  tone(10, 880.0, 400); delay(500);//A/
  
  //happy birthday
  tone(10, 784.0, 100); delay(500);//G/
  tone(10, 784.0, 200); delay(500);//G/
  tone(10, 740.0, 300); delay(500);//F#
  tone(10, 659.3, 400); delay(500);//E/
  
  //happy birthday to you
  tone(10, 1006.5, 100); delay(500);//C/
  tone(10, 1006.5, 100); delay(500);//C/
  tone(10, 987.8, 200); delay(500);//B/
  tone(10, 784.0, 200); delay(500);//G/
  tone(10, 880.0, 300); delay(500);//A/
  tone(10, 784.0, 400); delay(500);//G/
  
}
 
void song4()
{
