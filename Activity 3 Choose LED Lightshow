void LedLightShow4()
{
  //forward to right
  digitalWrite(13, HIGH);
  delay(60);
  digitalWrite(13, LOW);
  delay(50);
  
  digitalWrite(12, HIGH);
  delay(60);
  digitalWrite(12, LOW);
  delay(50);
  
  digitalWrite(11, HIGH);
  delay(60);
  digitalWrite(11, LOW);
  delay(50);
  
  digitalWrite(10, HIGH);
  delay(60);
  digitalWrite(10, LOW);
  delay(50);
  
  digitalWrite(9, HIGH);
  delay(60);
  digitalWrite(9, LOW);
  delay(50);
  
  digitalWrite(8, HIGH);
  delay(60);
  digitalWrite(8, LOW);
  delay(50);
  
  digitalWrite(7,HIGH);
  delay(60);
  digitalWrite(7, LOW);
  delay(50);
  
  digitalWrite(6,HIGH);
  delay(60);
  digitalWrite(6, LOW);
  delay(50);
  
  digitalWrite(5, HIGH);
  delay(60);
  digitalWrite(5, LOW);
  delay(50);
  
  digitalWrite(4, HIGH);
  delay(60);
  digitalWrite(4, LOW);
  delay(50);
  
  digitalWrite(3,HIGH);
  delay(60);
  digitalWrite(3, LOW);
  delay(50);
  
  digitalWrite(2,HIGH);
  delay(60);
  digitalWrite(2, LOW);
  delay(50);
  
  //reverse
  digitalWrite(2,HIGH);
  delay(60);
  digitalWrite(2, LOW);
  delay(50);
  
  digitalWrite(3,HIGH);
  delay(60);
  digitalWrite(3, LOW);
  delay(50);
  
  digitalWrite(4, HIGH);
  delay(60);
  digitalWrite(4, LOW);
  delay(50);
  
  digitalWrite(5, HIGH);
  delay(60);
  digitalWrite(5, LOW);
  delay(50);
  
  digitalWrite(6,HIGH);
  delay(60);
  digitalWrite(6, LOW);
  delay(50);

  digitalWrite(7,HIGH);
  delay(60);
  digitalWrite(7, LOW);
  delay(50);

  digitalWrite(8, HIGH);
  delay(60);
  digitalWrite(8, LOW);
  delay(50);
 
  digitalWrite(9, HIGH);
  delay(60);
  digitalWrite(9, LOW);
  delay(50);

  digitalWrite(10, HIGH);
  delay(60);
  digitalWrite(10, LOW);
  delay(50);

  digitalWrite(11, HIGH);
  delay(60);
  digitalWrite(11, LOW);
  delay(50);

  digitalWrite(12, HIGH);
  delay(60);
  digitalWrite(12, LOW);
  delay(50);

  digitalWrite(13, HIGH);
  delay(60);
  digitalWrite(13, LOW);
  delay(50);

  //intersect and disperse
  digitalWrite(13, HIGH);
  digitalWrite(2, HIGH);
  delay(60);
  digitalWrite(13, LOW);
  digitalWrite(2, LOW);
  delay(50);
  
  digitalWrite(12, HIGH);
  digitalWrite(3, HIGH);
  delay(60);
  digitalWrite(12, LOW);
  digitalWrite(3, LOW);
  delay(50);
  
  digitalWrite(11, HIGH);
  digitalWrite(4, HIGH);
  delay(60);
  digitalWrite(11, LOW);
  digitalWrite(4, LOW);
  delay(50);
  
  digitalWrite(10, HIGH);
  digitalWrite(5, HIGH);
  delay(60);
  digitalWrite(10, LOW);
  digitalWrite(5, LOW);
  delay(50);
  
  digitalWrite(9, HIGH);
  digitalWrite(6,HIGH);
  delay(60);
  digitalWrite(9, LOW);
  digitalWrite(6, LOW);
  delay(50);
  
  digitalWrite(8, HIGH);
  digitalWrite(7, HIGH);
  delay(60);
  digitalWrite(8, LOW);
  digitalWrite(7, LOW);
  delay(50);
  
  digitalWrite(7,HIGH);
  digitalWrite(8, HIGH);
  delay(60);
  digitalWrite(7, LOW);
  digitalWrite(8, LOW);
  delay(50);
  
  digitalWrite(6,HIGH);
  digitalWrite(9, HIGH);
  delay(60);
  digitalWrite(6, LOW);
  digitalWrite(9, LOW);
  delay(50);
  
  digitalWrite(5, HIGH);
  digitalWrite(10, HIGH);
  delay(60);
  digitalWrite(5, LOW);
  digitalWrite(10, LOW);
  delay(50);
  
  digitalWrite(4, HIGH);
  digitalWrite(11, HIGH);
  delay(60);
  digitalWrite(4, LOW);
  digitalWrite(11, LOW);
  delay(50);
  
  digitalWrite(3,HIGH);
  digitalWrite(12, HIGH);
  delay(60);
  digitalWrite(3, LOW);
  digitalWrite(12, LOW);
  delay(50);
  
  digitalWrite(2,HIGH);
  digitalWrite(13, HIGH);
  delay(60);
  digitalWrite(2, LOW);
  digitalWrite(13, LOW);
  delay(50);
}

void LedLightShow5()
{
  //threes to right
  digitalWrite(13, HIGH);
  digitalWrite(9, HIGH);
  digitalWrite(5, HIGH);
  delay(100);
  digitalWrite(13, LOW);
  digitalWrite(9, LOW);
  digitalWrite(5, LOW);
  delay(100);
  
  digitalWrite(12, HIGH);
  digitalWrite(8, HIGH);
  digitalWrite(4, HIGH);
  delay(100);
  digitalWrite(12, LOW);
  digitalWrite(8, LOW);
  digitalWrite(4, LOW);
  delay(100);
  
  digitalWrite(11, HIGH);
  digitalWrite(7, HIGH);
  digitalWrite(3, HIGH);
  delay(100);
  digitalWrite(11, LOW);
  digitalWrite(7, LOW);
  digitalWrite(3, LOW);
  delay(100);
  
  digitalWrite(10, HIGH);
  digitalWrite(6, HIGH);
  digitalWrite(2, HIGH);
  delay(100);
  digitalWrite(10, LOW);
  digitalWrite(6, LOW);
  digitalWrite(2, LOW);
  delay(100);
  
  //threes to left
  digitalWrite(10, HIGH);
  digitalWrite(6, HIGH);
  digitalWrite(2, HIGH);
  delay(100);
  digitalWrite(10, LOW);
  digitalWrite(6, LOW);
  digitalWrite(2, LOW);
  delay(100);
  
  digitalWrite(11, HIGH);
  digitalWrite(7, HIGH);
  digitalWrite(3, HIGH);
  delay(100);
  digitalWrite(11, LOW);
  digitalWrite(7, LOW);
  digitalWrite(3, LOW);
  delay(100);
  
  digitalWrite(12, HIGH);
  digitalWrite(8, HIGH);
  digitalWrite(4, HIGH);
  delay(100);
  digitalWrite(12, LOW);
  digitalWrite(8, LOW);
  digitalWrite(4, LOW);
  delay(100);
  
  digitalWrite(13, HIGH);
  digitalWrite(9, HIGH);
  digitalWrite(5, HIGH);
  delay(100);
  digitalWrite(13, LOW);
  digitalWrite(9, LOW);
  digitalWrite(5, LOW);
  delay(100);
  
}


//MAIN PROGRAM
void loop() {

while (Serial.available()==0) {
}

int userinput = Serial.parseInt();

switch (userinput) {
case 1:
LedLightShow1();
break;

case 2:
LedLightShow2();
break;

case 3:
LedLightShow3();
break;

case 4:
LedLightShow4();
break;

case 5:
LedLightShow5();
break;

default:
Serial.println("Selected Feature Does Not Exist");
}
}
